{
  "folders":
  [
    {
      "file_exclude_patterns":
      [
        "*.doc",
        "*.docx",
        "*.xlsx",
        "*.pptx",
        "*.tar.gz",
        "*.zip",
        "*.pdf",
        "*.log"
      ],
      "name": "calculorie",
      "path": "."
    }
  ],
  "SublimeLinter":
  {
    "linters":
    {
      "rubocop":
      {
        "@disable": false,
        "args":
        [
          "--config",
          "${project}/mtt/EzCall/.rubocop.yml"
        ],
        "excludes":
        [
        ],
        "use_bundle_exec": true
      },
      "ruby":
      {
        "@disable": false,
        "args":
        [
        ],
        "excludes":
        [
        ]
      }
    }
  },
  "settings":
  {
    "default_encoding": "UTF-8",
    "detect_indentation": false,
    "encoding": "UTF-8",
    "fallback_encoding": "UTF-8",
    "font_size": 14,
    "rulers":
    [
      100
    ],
    "show_encoding": true,
    "tab_size": 2,
    "translate_tabs_to_spaces": true,
    "use_tab_stops": true
  }
}
